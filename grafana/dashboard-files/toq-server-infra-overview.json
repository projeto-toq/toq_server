    "panels": [
        {
            "id": 1,
            "title": "Host CPU Utilization",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "100 * sum(rate(system_cpu_time{service=~\"$service\", state!=\"idle\"}[$__rate_interval])) / sum(rate(system_cpu_time{service=~\"$service\"}[$__rate_interval]))",
                    "legendFormat": "CPU %",
                    "refId": "A"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 0,
                "y": 0
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
                    "decimals": 2
                },
                "overrides": []
            },
            "options": {
                "legend": {
                    "displayMode": "list",
                    "placement": "bottom",
                    "showLegend": true
                }
            }
        },
        {
            "id": 2,
            "title": "Host Memory Usage",
            "type": "timeseries",
            "datasource": {
                "type": "prometheus",
                "uid": "prometheus"
            },
            "targets": [
                {
                    "expr": "100 * sum(system_memory_usage{service=~\"$service\", state=\"used\"}) / sum(system_memory_usage{service=~\"$service\", state=\"total\"})",
                    "legendFormat": "Memory %",
                    "refId": "A"
                }
            ],
            "gridPos": {
                "h": 8,
                "w": 12,
                "x": 12,
                "y": 0
            },
            "fieldConfig": {
                "defaults": {
                    "unit": "percent",
